package team.ElectricityPatrolSys.entity;

import java.util.Date;

public class Bug {

	private String bug_id; // 缺陷id

	private SysUser sysUser; // 发现人id

	private Date find_time; // 发现时间

	private Tower tower; // 杆塔id

	private PollTask pollTask; // 巡检任务

	private BeHeTask beHeTask; // 消缺任务
	
	private String behe_task_id;

	private BugLeave bugLeave; // 缺陷等级

	private String des; // 缺陷描述

	private BugType bugType; // 缺陷类型

	private String cir_tower;

	public Bug(String bug_id, SysUser sysUser, Date find_time, Tower tower,
			BugLeave bugLeave, String des, BugType bugType, String cir_tower) {
		this.bug_id = bug_id;
		this.sysUser = sysUser;
		this.find_time = find_time;
		this.tower = tower;
		this.bugLeave = bugLeave;
		this.des = des;
		this.bugType = bugType;
		this.cir_tower = cir_tower;
	}

	public PollTask getPollTask() {
		return pollTask;
	}

	public void setPollTask(PollTask pollTask) {
		this.pollTask = pollTask;
	}

	public BeHeTask getBeHeTask() {
		return beHeTask;
	}

	public void setBeHeTask(BeHeTask beHeTask) {
		this.beHeTask = beHeTask;
	}

	public Bug() {
	}

	public String getBug_id() {
		return bug_id;
	}

	public void setBug_id(String bug_id) {
		this.bug_id = bug_id;
	}

	public SysUser getSysUser() {
		return sysUser;
	}

	public void setSysUser(SysUser sysUser) {
		this.sysUser = sysUser;
	}

	public Date getFind_time() {
		return find_time;
	}

	public void setFind_time(Date find_time) {
		this.find_time = find_time;
	}

	public Tower getTower() {
		return tower;
	}

	public void setTower(Tower tower) {
		this.tower = tower;
	}

	public BugLeave getBugLeave() {
		return bugLeave;
	}

	public void setBugLeave(BugLeave bugLeave) {
		this.bugLeave = bugLeave;
	}

	public String getDes() {
		return des;
	}

	public void setDes(String des) {
		this.des = des;
	}

	public BugType getBugType() {
		return bugType;
	}

	public void setBugType(BugType bugType) {
		this.bugType = bugType;
	}

	public String getCir_tower() {
		return cir_tower;
	}

	public void setCir_tower(String cir_tower) {
		this.cir_tower = cir_tower;
	}

	/**
	 * behe_task_id的getter方法
	 * 创建时间：2015-1-21 下午9:12:01 <br>
	 * @return the behe_task_id
	 */
	
	public String getBehe_task_id() {
		return behe_task_id;
	}

	/**
	 * behe_task_id的setter方法
	 * 创建时间：2015-1-21 下午9:12:01 <br>
	 * @param behe_task_id the behe_task_id to set
	 */
	
	public void setBehe_task_id(String behe_task_id) {
		this.behe_task_id = behe_task_id;
	}

	/** 
	  * 创建时间：2015-1-21 下午9:12:06 <br>
	  * <p>Description: </p> 
	  * @param bug_id
	  * @param sysUser
	  * @param find_time
	  * @param tower
	  * @param pollTask
	  * @param beHeTask
	  * @param behe_task_id
	  * @param bugLeave
	  * @param des
	  * @param bugType
	  * @param cir_tower 
	*/
	
	public Bug(String bug_id, SysUser sysUser, Date find_time, Tower tower,
			PollTask pollTask, BeHeTask beHeTask, String behe_task_id,
			BugLeave bugLeave, String des, BugType bugType, String cir_tower) {
		super();
		this.bug_id = bug_id;
		this.sysUser = sysUser;
		this.find_time = find_time;
		this.tower = tower;
		this.pollTask = pollTask;
		this.beHeTask = beHeTask;
		this.behe_task_id = behe_task_id;
		this.bugLeave = bugLeave;
		this.des = des;
		this.bugType = bugType;
		this.cir_tower = cir_tower;
	}

	
}
