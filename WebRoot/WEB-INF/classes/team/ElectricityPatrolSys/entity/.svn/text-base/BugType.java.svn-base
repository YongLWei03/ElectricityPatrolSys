package team.ElectricityPatrolSys.entity;

public class BugType {

	private String bug_type_id; // 缺陷类型id

	private String bug_type_name; // 名

	private StatusComm statusComm; // 状态

	public BugType(String bug_type_id, String bug_type_name,
			StatusComm statusComm) {
		this.bug_type_id = bug_type_id;
		this.bug_type_name = bug_type_name;
		this.statusComm = statusComm;
	}

	public BugType() {
	}

	public String getBug_type_id() {
		return bug_type_id;
	}

	public void setBug_type_id(String bug_type_id) {
		this.bug_type_id = bug_type_id;
	}

	public String getBug_type_name() {
		return bug_type_name;
	}

	public void setBug_type_name(String bug_type_name) {
		this.bug_type_name = bug_type_name;
	}

	public StatusComm getStatusComm() {
		return statusComm;
	}

	public void setStatusComm(StatusComm statusComm) {
		this.statusComm = statusComm;
	}

}
