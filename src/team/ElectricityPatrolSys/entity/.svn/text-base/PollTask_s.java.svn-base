package team.ElectricityPatrolSys.entity;

import java.util.Date;

public class PollTask_s {

	private String task_id_s; // 巡检任务id

	private String task_name_s; // 任务名

	private String circuit_s; // 线路id

	private String start_tower_id_s; // 杆塔起始编号

	private String end_tower_id_s;// 杆塔结束编号

	private String tower_id; // 杆塔id

	private String bugLeave; // 缺陷等级

	private String bugType; // 缺陷类型

	private Date find_time; // 发现时间

	private String des; // 缺陷描述

	public PollTask_s(String task_id_s, String task_name_s, String circuit_s,
			String start_tower_id_s, String end_tower_id_s, String tower_id,
			String bugLeave, String bugType, Date find_time, String des) {
		this.task_id_s = task_id_s;
		this.task_name_s = task_name_s;
		this.circuit_s = circuit_s;
		this.start_tower_id_s = start_tower_id_s;
		this.end_tower_id_s = end_tower_id_s;
		this.tower_id = tower_id;
		this.bugLeave = bugLeave;
		this.bugType = bugType;
		this.find_time = find_time;
		this.des = des;
	}

	public String getTask_id_s() {
		return task_id_s;
	}

	public void setTask_id_s(String task_id_s) {
		this.task_id_s = task_id_s;
	}

	public String getTask_name_s() {
		return task_name_s;
	}

	public void setTask_name_s(String task_name_s) {
		this.task_name_s = task_name_s;
	}

	public String getCircuit_s() {
		return circuit_s;
	}

	public void setCircuit_s(String circuit_s) {
		this.circuit_s = circuit_s;
	}

	public String getStart_tower_id_s() {
		return start_tower_id_s;
	}

	public void setStart_tower_id_s(String start_tower_id_s) {
		this.start_tower_id_s = start_tower_id_s;
	}

	public String getEnd_tower_id_s() {
		return end_tower_id_s;
	}

	public void setEnd_tower_id_s(String end_tower_id_s) {
		this.end_tower_id_s = end_tower_id_s;
	}

	public String getTower_id() {
		return tower_id;
	}

	public void setTower_id(String tower_id) {
		this.tower_id = tower_id;
	}

	public String getBugLeave() {
		return bugLeave;
	}

	public void setBugLeave(String bugLeave) {
		this.bugLeave = bugLeave;
	}

	public String getBugType() {
		return bugType;
	}

	public void setBugType(String bugType) {
		this.bugType = bugType;
	}

	public Date getFind_time() {
		return find_time;
	}

	public void setFind_time(Date find_time) {
		this.find_time = find_time;
	}

	public String getDes() {
		return des;
	}

	public void setDes(String des) {
		this.des = des;
	}

}
