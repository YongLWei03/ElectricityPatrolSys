package team.ElectricityPatrolSys.entity;

public class BeheTaskUser {

	private String btu_id;

	private String behe_task_id;

	private String sys_user_id;
	
	private SysUser sysUser;

	public BeheTaskUser() {
	}

	public BeheTaskUser(String btu_id, String behe_task_id, String sys_user_id) {
		super();
		this.btu_id = btu_id;
		this.behe_task_id = behe_task_id;
		this.sys_user_id = sys_user_id;
	}

	public String getBtu_id() {
		return btu_id;
	}

	public void setBtu_id(String btu_id) {
		this.btu_id = btu_id;
	}

	public String getBehe_task_id() {
		return behe_task_id;
	}

	public void setBehe_task_id(String behe_task_id) {
		this.behe_task_id = behe_task_id;
	}

	public String getSys_user_id() {
		return sys_user_id;
	}

	public void setSys_user_id(String sys_user_id) {
		this.sys_user_id = sys_user_id;
	}

	public SysUser getSysUser() {
		return sysUser;
	}

	public void setSysUser(SysUser sysUser) {
		this.sysUser = sysUser;
	}

	public BeheTaskUser(String btu_id, String behe_task_id, String sys_user_id,
			SysUser sysUser) {
		super();
		this.btu_id = btu_id;
		this.behe_task_id = behe_task_id;
		this.sys_user_id = sys_user_id;
		this.sysUser = sysUser;
	}

	
}
